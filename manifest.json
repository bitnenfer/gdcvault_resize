{
    "manifest_version": 3,
    "name": "GDCValut Resize",
    "version": "1.0",
    "permissions": ["tabs", "activeTab"],
    "content_scripts": [
      {
        "matches": ["*://gdcvault.com/*"],
        "js": ["content.js"]
      }
    ],
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
  